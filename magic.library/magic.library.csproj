<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>magic.library</PackageId>
    <Authors>Thomas Hansen</Authors>
    <Company>Thomas Hansen</Company>
    <Copyright>Thomas Hansen, thomas@gaiasoul.com</Copyright>
    <Description>Helper project for Magic to wire up everything in Magic.</Description>
    <PackageProjectUrl>https://github.com/polterguy/magic.library</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/polterguy/magic.library/master/LICENSE</PackageLicenseUrl>
    <PackageIconUrl>https://polterguy.github.io/favicon.ico</PackageIconUrl>
    <PackOnBuild>true</PackOnBuild>
    <PackageVersion>1.1.5</PackageVersion>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/polterguy/magic.library.git</RepositoryUrl>
    <PackageTags>string manipulation</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin/Release/netstandard2.0/magic.library.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="magic.lambda" Version="1.1.6" />
    <PackageReference Include="magic.lambda.config" Version="1.1.5" />
    <PackageReference Include="magic.lambda.crypto" Version="1.1.5" />
    <PackageReference Include="magic.lambda.http" Version="1.1.5" />
    <PackageReference Include="magic.lambda.hyperlambda" Version="1.1.5" />
    <PackageReference Include="magic.lambda.io" Version="1.1.5" />
    <PackageReference Include="magic.lambda.json" Version="1.1.5" />
    <PackageReference Include="magic.lambda.logging" Version="1.1.6" />
    <PackageReference Include="magic.lambda.math" Version="1.1.5" />
    <PackageReference Include="magic.lambda.mysql" Version="1.1.6" />
    <PackageReference Include="magic.lambda.slots" Version="1.1.6" />
    <PackageReference Include="magic.lambda.strings" Version="1.1.5" />
    <PackageReference Include="magic.io" Version="1.1.5" />
    <PackageReference Include="magic.lambda.mssql" Version="1.1.7" />
    <PackageReference Include="magic.lambda.auth" Version="1.1.6" />
    <PackageReference Include="magic.endpoint" Version="1.1.6" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="internals\" />
  </ItemGroup>
</Project>
